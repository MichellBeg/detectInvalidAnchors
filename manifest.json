{
    "manifest_version": 3,
    "name": "Detect Invalid Anchors",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "css": ["hilight.css"],
          "js": ["content.js"]
        }
      ]
}